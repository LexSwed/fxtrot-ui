{
  "name": "@fxtrot/ui",
  "description": "UI library for Fxtrot initiatives",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "main": "./dist/fxtrot-ui.cjs",
  "module": "./dist/fxtrot-ui.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/fxtrot-ui.js",
      "require": "./dist/fxtrot-ui.cjs"
    },
    "./dist/tailwind.preset.cjs": "./dist/tailwind.preset.cjs",
    "./dist/styles.css": "./dist/styles.css"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/LexSwed/fxtrot-ui"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:types && npm run build:preset",
    "build:lib": "vite build --mode production",
    "build:types": "tsc --project tsconfig.lib.json",
    "build:preset": "vite build --config vite.tailwind-preset-config.mjs",
    "dev": "npm run docs:dev",
    "lint": "next lint --dir pages --dir src",
    "typecheck": "tsc --noEmit --project ./tsconfig.without-next-modifications.json",
    "docs:dev": "next dev -p 3001",
    "docs:build": "next build",
    "docs:start": "next start",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,md,mdx,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.0.2",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-direction": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-id": "1.0.0",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-portal": "^1.0.2",
    "@radix-ui/react-roving-focus": "^1.0.3",
    "@radix-ui/react-select": "^1.2.1",
    "@radix-ui/react-tabs": "^1.0.3",
    "@radix-ui/react-toggle": "^1.0.2",
    "@radix-ui/react-toggle-group": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.5",
    "@radix-ui/react-visually-hidden": "^1.0.2",
    "@tw-classed/core": "^1.4.0",
    "clsx": "^1.2.1",
    "react-keyed-flatten-children": "^1.3.0"
  },
  "devDependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.2.4",
    "@stitches/react": "^1.2.8",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^8.1.0",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "next": "^13.2.4",
    "next-seo": "^5.15.0",
    "pinst": "^3.0.0",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.2.0",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "prism-react-renderer": "^1.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-live": "^3.2.0",
    "rehype-slug": "^5.0.1",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx-code-meta": "^2.0.0",
    "remark-mdx-frontmatter": "^2.0.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "typescript-plugin-css-modules": "^5.0.0",
    "vite": "^4.2.1"
  },
  "peerDependencies": {
    "@heroicons/react": "^2.0.16",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "ui",
    "design-system",
    "react-components",
    "uikit",
    "accessible",
    "components",
    "library",
    "design-system"
  ],
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "quoteProps": "consistent"
  },
  "browserslist": [
    "> 2%"
  ],
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
